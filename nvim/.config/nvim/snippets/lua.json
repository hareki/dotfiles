{
  "function": {
    "prefix": "fu",
    "body": [
      "function ${1:name}($2)",
      "\t${0:-- code}",
      "end"
    ]
  },
  "foreach": {
    "prefix": "forp",
    "body": [
      "for i, ${1:x} in pairs(${2:table}) do",
      "\t$0",
      "end"
    ]
  },
  "if": {
    "prefix": "if",
    "body": [
      "if ${1:true} then",
      "\t$0",
      "end"
    ]
  },
  "elseif": {
    "prefix": "elif",
    "body": [
      "elseif ${1:true} then",
      "\t$0"
    ]
  }
}
